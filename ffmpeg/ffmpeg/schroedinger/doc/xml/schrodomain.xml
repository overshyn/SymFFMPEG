<refsect2 id="SchroMemoryDomain" role="struct">
<title>SchroMemoryDomain</title>
<indexterm zone="SchroMemoryDomain"><primary>SchroMemoryDomain</primary></indexterm><programlisting>typedef struct {
  SchroMutex * mutex;

  unsigned int flags;

  void *(*alloc) (int size);
  void *(*alloc_2d) (int depth, int width, int height);
  void (*free) (void *ptr, int size);

  struct {
    unsigned int flags;
    void *ptr;
    int size;
    void *priv;
  } slots[SCHRO_MEMORY_DOMAIN_SLOTS];
} SchroMemoryDomain;
</programlisting>
